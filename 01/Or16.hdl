/**
 * 16-bit bitwise Or gate:
 * for i = 0, ..., 15:
 * out[i] = a[i] Or b[i] 
 */
CHIP Or16 {
    IN a[16], b[16];
    OUT out[16];

    PARTS:
    Not16(in=a, out=an);
    Not16(in=b, out=bn);
    And16(a=an, b=bn, out=x);
    Not16(in=x, out=out);
}